{
  "name": "react-18-github-actions",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "tsc && vite build",
    "eslint-fix": "eslint . --ext ts,tsx --report-unused-disable-directives --ignore-path .gitignore --fix",
    "eslint-check": "eslint . --ext ts,tsx --report-unused-disable-directives --ignore-path .gitignore --max-warnings 0",
    "stylelint-fix": "stylelint src/**/*.css src/**/*.scss --fix",
    "stylelint-fix-twice": "npm run stylelint-fix && npm run stylelint-fix",
    "preview": "vite preview",
    "format": "prettier --write src/"
  },
  "dependencies": {
    "react": "^18.2.0",
    "react-dom": "^18.2.0"
  },
  "devDependencies": {
    "@types/react": "^18.2.15",
    "@types/react-dom": "^18.2.7",
    "@typescript-eslint/eslint-plugin": "^6.0.0",
    "@typescript-eslint/parser": "^6.0.0",
    "@vitejs/plugin-react": "^4.0.3",
    "eslint": "^8.45.0",
    "eslint-config-prettier": "^9.0.0",
    "eslint-plugin-prettier": "^5.0.1",
    "eslint-plugin-react-hooks": "^4.6.0",
    "eslint-plugin-react-refresh": "^0.4.3",
    "prettier": "^3.0.3",
    "sass": "^1.69.3",
    "stylelint": "^15.10.3",
    "stylelint-config-prettier-scss": "^1.0.0",
    "stylelint-config-recess-order": "^4.3.0",
    "stylelint-config-recommended": "^13.0.0",
    "stylelint-config-recommended-scss": "^13.0.0",
    "stylelint-prettier": "^4.0.2",
    "typescript": "^5.0.2",
    "vite": "^4.4.5"
  }
}
